{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/room/([^/]+)",
      "dest": "/apps/web/room/[code]?code=$1"
    },
    {
      "src": "/s/([^/]+)",
      "dest": "/apps/web/s/[shareUrl]?shareUrl=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ]
}
